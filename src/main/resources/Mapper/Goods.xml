<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.take.takeDemo.Dao.GoodsDao">

    <!--返回集 -->
    <resultMap id="backMap"
               type="com.take.takeDemo.Entity.Goods">
    </resultMap>

    <!--检索 -->
    <select id="findAll" resultMap="backMap">
        select * from goods
    </select>

    <!--添加物品-->
    <insert id="insertGoods" parameterType="Goods">
        insert into goods (goodsName,userId,goodsPlace,goodsPublic,goodsCode,saveTimes,placeTime,takeTime,goodsType,frequency,goodsPhoto,goodsDescribe)
        values (#{goodsName},#{userId},#{goodsPlace},#{goodsPublic},#{goodsCode},#{saveTimes},#{placeTime},#{takeTime},#{goodsType},#{frequency},#{goodsPhoto},#{goodsDescribe})
    </insert>

</mapper>