<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.take.takeDemo.Dao.FieldDao">

    <!--返回集 -->
    <resultMap id="backMap"
               type="com.take.takeDemo.Entity.Field">
    </resultMap>

    <!--检索 -->
    <select id="findAll" resultMap="backMap">
        select * from Field
    </select>

    <!--注册-->
    <insert id="insertField" parameterType="Field">
        insert into Artist values (#{fieldname},#{fieldadress},#{fieldcall},#{musictype},#{fieldtime},#{fieldtype})
    </insert>

</mapper>